{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    },
    "noEmitOnError": false,
    "noImplicitAny": false,
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "context/UserDetailContext.jsx", "components/custom/Setting.jsx", "components/custom/Canvas.jsx", "components/custom/EditorHeader.jsx", "components/custom/ElementsSidebar.jsx", "components/custom/ElementLayoutCard.jsx", "constants/layout.js", "provider/Provider.tsx",],
  "exclude": ["node_modules"]
}
